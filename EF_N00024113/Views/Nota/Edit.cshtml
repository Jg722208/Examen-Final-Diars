@{ Layout = null;}<form id="editNota" onsubmit="editar(event)" action="/Nota/Edit" enctype="multipart/form-data" method="post">    <input id="mtxtId" type="hidden" name="Id" />    <div class="form-group">        <label>Titulo</label>        <input id="mtxTitulo" type="text" class="form-control" name="Titulo" placeholder="Titulo" />        @Html.ValidationMessage("Titulo")    </div>    <div class="form-group">        <label>Contenido</label>    </div>    <textarea id="mtxContedido" name="Contenido" rows="10" cols="50" value ="@ViewBag.contenido"></textarea>    <br />    @Html.ValidationMessage("Contenido")</form>  